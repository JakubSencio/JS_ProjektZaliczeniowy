@page
@model RazorPagesApp.Pages.SoapClientModel
@{
    ViewData["Title"] = "SOAP Client";
}

<h2>@ViewData["Title"]</h2>

<form method="post">
    <button type="submit">Call SOAP Service</button>
</form>

@if (Model.Response != null)
{
    <div>
        <h3>Response:</h3>
        <pre>@Model.Response</pre>
    </div>
}
